{
  "name": "ferreteria",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:server": "set NODE_OPTIONS=--max-old-space-size=8192 && next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "share": "npx tunnelmole 3000",
    "migrate:fresh": "(if exist prisma\\migrations (rmdir /s /q prisma\\migrations)) && npx prisma migrate reset --force && npx prisma migrate dev --name init",
    "migrate:mysql": "powershell -ExecutionPolicy Bypass -File migrate-to-mysql.ps1",
    "db:reset": "npx prisma migrate reset --force",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "^1.0.2",
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-pdf/renderer": "^4.3.0",
    "@tanstack/react-query": "^5.83.0",
    "ag-grid-react": "^34.0.1",
    "antd": "^5.26.3",
    "argon2": "^0.43.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.525.0",
    "mysql2": "^3.15.3",
    "next": "^15.5.7",
    "next-auth": "^5.0.0-beta.29",
    "react": "^19.0.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.1.0",
    "size-limit": "^12.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animated": "^2.0.0",
    "use-debounce": "^10.0.6",
    "uuid": "^11.1.0",
    "xlsx-js-style": "^1.2.0",
    "zod": "^3.25.76",
    "zod-prisma-types": "^3.2.4",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "prisma": "^6.12.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5"
  },
  "prisma": {
    "schema": "./prisma"
  }
}
